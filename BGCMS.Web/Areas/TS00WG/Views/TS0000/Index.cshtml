                                <div class="layui-input-inline">
                                    <input type="text" name="date" id="date" lay-verify="date" autocomplete="off" class="layui-input">
                                </div>
                            </div>
                            <div class="layui-inline">
                                <label class="layui-form-label">代码名称</label>
